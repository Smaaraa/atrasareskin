---
import { Image } from 'astro:assets';
import BaseLayout from "../layouts/BaseLayout.astro";
import test from "../images/test.jpg";
const { title, images, alt, urls, width, height } = Astro.props;
---

<BaseLayout title={title} is404Page={false}>
  <div
    id="homepage"
    class="flex justify-center items-center"
    data-images={images}
    data-alt={alt}
    data-urls={urls}
    data-width={JSON.stringify(width)}
    data-height={JSON.stringify(height)}
  >
    <Image class="sm:h-[600px] lg:h-[700px] xl:h-[1000px] object-contain w-full object-center"
    src={test} alt="A bird sitting on a nest of eggs." />
    <slot />
  </div>
</BaseLayout>
<script src="../scripts/homePage.js"></script>
